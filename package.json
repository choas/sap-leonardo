{
  "name": "sap-leonardo",
  "version": "0.5.5",
  "description": "NPM module for SAP Leonardo Machine Learning Foundation - Functional Services https://api.sap.com/package/SAPLeonardoMLFunctionalServices",
  "main": "dist/index.js",
  "scripts": {
    "build": "./node_modules/.bin/tsc",
    "test": "npm run build && ./node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha --report lcovonly -- -s 10000 -t 10000 dist/test/test_*.js && cat ./coverage/lcov.info | ./node_modules/.bin/coveralls",
    "coverage": "npm run build && ./node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha  -- -s 10000 -t 10000 dist/test/test_*.js",
    "lint": "./node_modules/.bin/tslint src/**/*.ts && ./node_modules/.bin/eslint examples/*.js"
  },
  "author": "Lars Gregori <github@larsgregori.de>",
  "license": "Apache-2.0",
  "repository": {
    "type": "https",
    "url": "https://github.com/choas/sap-leonardo.git"
  },
  "files": [
    "dist/",
    "README.md"
  ],
  "keywords": [
    "sap",
    "leonardo",
    "machine learning",
    "ml",
    "image",
    "text",
    "classification",
    "segmentation",
    "ocr",
    "optical",
    "character",
    "recognition",
    "face",
    "human",
    "language",
    "detection",
    "translation"
  ],
  "dependencies": {
    "es6-promise": "^4.2.5",
    "request": "^2.88.0"
  },
  "devDependencies": {
    "@types/chai": "^4.1.6",
    "@types/mocha": "^5.2.5",
    "@types/request": "^2.47.1",
    "chai": "^4.2.0",
    "coveralls": "^3.0.2",
    "eslint": "^5.7.0",
    "istanbul": "^0.4.5",
    "mocha": "^5.2.0",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "typescript": "^3.1.3"
  }
}
